public void selectionSort(int[] nums) {
        int length = nums.length;
        int min_index;
        int temp;
        for(int i = 0; i < length -1;i++) {
            min_index = i;
            for(int j = i+1; j < length; j++) {
                if(nums[j] < nums[min_index]) min_index = j;
            }
            temp = nums[i];
            nums[i] = nums[min_index];
            nums[min_index] = temp;
        }
    }
